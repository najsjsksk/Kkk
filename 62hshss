    end
})

-- // Label hi·ªÉn th·ªã k·∫øt qu·∫£
local statusLabel = Tab:AddLabel("‚è≥ Ch∆∞a ki·ªÉm tra...")

-- // H√†m g·ª≠i request (cho exploit)
local function sendRequest(url, data)
    local req = http_request or request or syn and syn.request
    if not req then
        return nil, "‚ùå Exploit c·ªßa b·∫°n kh√¥ng h·ªó tr·ª£ http_request"
    end

    local response = req({
        Url = url,
        Method = "POST",
        Headers = {
            ["Content-Type"] = "application/json"
        },
        Body = data
    })

    return response, nil
end

-- // N√∫t ki·ªÉm tra key
